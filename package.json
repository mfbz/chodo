{
	"name": "chodo",
	"version": "1.0.0",
	"description": "The to-do list dapp to build your next dream project.",
	"author": "mfbz",
	"testnetDefaultChannel": "v1.3.15",
	"scripts": {
		"cli:account": "node ./cli/account.js",
		"cli:account-add": "node ./cli/account.js --add",
		"cli:balance": "node ./cli/balance.js",
		"cli:dump": "node ./cli/dump.js",
		"cli:dump-pubkey": "node ./cli/dump.js --pubkey=",
		"cli:deploy": "node ./cli/deploy.js",
		"cli:launch": "npm run localnet:update && npm run localnet:up && npm run cli:account && npm run program:build && npm run cli:deploy",
		"cli:clean-store": "rm -rf store",
		"program:build": "./program/do.sh build",
		"program:clean": "./program/do.sh clean && rm -rf ./dist && npm run cli:clean-store",
		"program:test": "./program/do.sh test",
		"bpf-sdk:update": "solana-bpf-sdk-install node_modules/@solana/web3.js && npm run program:clean",
		"web:start": "craco start",
		"web:build": "craco build",
		"web:test": "craco test",
		"localnet:update": "solana-localnet update",
		"localnet:up": "npm run cli:clean-store && set -x; solana-localnet down; set -e; solana-localnet up",
		"localnet:down": "solana-localnet down",
		"localnet:logs": "solana-localnet logs -f",
		"preinstall": "npx npm-force-resolutions",
		"postinstall": "npm run bpf-sdk:update && cargo update --manifest-path=./program/Cargo.toml"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"@craco/craco": "^6.1.1",
		"@ledgerhq/hw-transport-webusb": "^5.43.0",
		"@project-serum/borsh": "^0.1.0",
		"@project-serum/sol-wallet-adapter": "^0.1.5",
		"@solana/web3.js": "0.87.2",
		"antd": "^4.12.3",
		"bn.js": "^5.1.3",
		"buffer-layout": "^1.2.0",
		"craco-less": "^1.17.1",
		"eventemitter3": "^4.0.7",
		"moment": "^2.29.1",
		"react": "^17.0.1",
		"react-cool-dimensions": "^1.3.1",
		"react-dom": "^17.0.1",
		"react-responsive": "^8.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.2",
		"rxjs": "^6.6.3",
		"soprox-abi": "0.0.3",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@testing-library/react": "^11.2.2",
		"@testing-library/react-hooks": "^5.0.3",
		"@testing-library/user-event": "^12.7.1",
		"@types/bn.js": "^5.1.0",
		"@types/jest": "^26.0.20",
		"@types/ledgerhq__hw-transport": "^4.21.3",
		"@types/ledgerhq__hw-transport-webusb": "^4.70.1",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@types/react-responsive": "^8.0.2",
		"@types/react-router-dom": "^5.1.6",
		"@typescript-eslint/eslint-plugin": "^4.4.1",
		"@typescript-eslint/parser": "^4.4.1",
		"eslint": "^7.20.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-rxjs": "3.1.0",
		"prettier": "^2.2.1",
		"typescript": "^4.1.5"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"resolutions": {
		"buffer": "6.0.1"
	}
}
